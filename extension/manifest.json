{
   "name": "Toggle StreetView Converage",
   "short_name": "Toggle StreetView Coverage",
   "version": "1.0.1",
   "description": "Toggles available StreetView Coverage on Google Maps",
   "background": {
      "scripts": [ "js/backgroundpage.js" ],
      "persistent": false
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon_19.png",
         "38": "img/icon_38.png"
      },
      "default_title": "Toggle StreetView Converage"
   },
   "commands": {
      "_execute_browser_action": {
         "description": "Toggle StreetView Converage",
         "suggested_key": {
            "chromeos": "Alt+S",
            "linux": "Alt+S",
            "mac": "Option+S",
            "windows": "Alt+S"
         }
      }
   },
   "permissions": [ "activeTab" ],
   "icons": {
      "128": "img/icon_38.png",
      "16": "img/icon_16.png",
      "48": "img/icon_128.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "22",
   "update_url": "https://clients2.google.com/service/update2/crx"
}
